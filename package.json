{
  "name": "dengine",
  "version": "1.0.0",
  "description": "A data automation app",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.bundle.js",
    "copy-xmls": "cpx \"src/XMLs/*\" build/XMLs/",
    "server:build": "npm run copy-xmls && tsc --watch",
    "server:run": "nodemon build/index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server:*\" \"npm run client\"",
    "dev-server": "concurrently \"npm run server:*\"",
    "tsc-build": "tsc --b",
    "bundle": "parcel build build/index.js --target node -d build -o index.bundle.js --no-cache --no-source-maps",
    "heroku-postbuild": "npm i --only=dev --prefix client && npm i --prefix client && npm run build --prefix client && npm i --only=dev && npm i && npm run copy-xmls && npm run tsc-build && npm run bundle",
    "build-app": "npm run build --prefix client && npm run copy-xmls && npm run tsc-build && npm run bundle"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "curlrequest": "^1.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fast-xml-parser": "^3.16.0",
    "flat": "^5.0.0",
    "fs-extra": "^9.0.0",
    "helmet": "^3.22.0",
    "joi": "^14.3.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.10",
    "multiparty": "^4.2.1",
    "node-xlsx": "^0.15.0",
    "promise-memoize": "^1.2.1",
    "random-string-generator": "^1.0.0",
    "simple-crypto-js": "^2.2.0",
    "typescript": "~3.8.3",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/flat": "5.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/he": "^1.1.1",
    "@types/helmet": "0.0.46",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.7.14",
    "@types/multiparty": "0.0.32",
    "@types/node": "^13.13.4",
    "@types/promise-memoize": "^1.2.0",
    "@types/winston": "^2.4.4",
    "@types/xml-js": "^1.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "supertest": "^4.0.2",
